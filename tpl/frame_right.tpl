<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <title>欢迎界面</title>
  <link href="css/skin.css" rel="stylesheet" type="text/css" />
  <style type="text/css">
  <!--
  body {
    margin-left: 0px;
    margin-top: 0px;
    margin-right: 0px;
    margin-bottom: 0px;
    background-color: #EEF2FB;
  }
  -->
  </style>

</head>
<body>
  <table width="100%" border="0" cellpadding="0" cellspacing="0">
    <tr>
      <td width="17" valign="top" style=" vertical-align:middle"  background="images/mail_leftbg.gif"><img src="images/left-top-right.gif" width="17" height="29" /></td>
      <td valign="top" background="images/content-bg.gif">
       <div class="radiusbox">欢迎界面</div>
     </td>
     <td width="16" valign="top" background="images/mail_rightbg.gif"><img src="images/nav-right-bg.gif" width="16" height="29" /></td>
   </tr>
   <tr>
    <td valign="middle" background="images/mail_leftbg.gif">&nbsp;</td>
    <td valign="top" bgcolor="#F7F8F9">
      <table width="100%" border="0" align="center" cellpadding="0" cellspacing="0">
        <tr>
          <td colspan="2" valign="top" height="3"></td>
        </tr>
        <tr>
          <td colspan="2" valign="top" height="100%" >
            <table width="100%" height="250" border="0" cellpadding="0" cellspacing="0" class="line_table">
              <tr>
                <td colspan="2" background="images/news-title-bg.gif" class="left_bt2">&nbsp;欢迎您使用后台管理系统</td>
              </tr>
              <tr>
                <td  valign="top" colspan="2">
                 <table width="99%" border="0" cellspacing="1" cellpadding="0"  bgcolor="#F0F0F0" align="center">                        
                  <tr >
                    <td  bgcolor="#FFFFFF" class="left_txt" width="140">登录帐号：</td>
                    <td  bgcolor="#FFFFFF" class="left_txt"> &nbsp;{# $user_name #}</td>
                  </tr>
                  <tr >
                    <td bgcolor="#FFFFFF" width="140" class="left_txt">帐号ID：</td>
                    <td  bgcolor="#FFFFFF" class="left_txt">&nbsp;{# $account #}</td>
                  </tr>

                  <tr >
                    <td  bgcolor="#FFFFFF" width="140" class="left_txt">创建时间：</td>
                    <td bgcolor="#FFFFFF" class="left_txt">&nbsp;{# $create_time #}</td>
                  </tr>
                  <tr >
                    <td  bgcolor="#FFFFFF"  width="140" class="left_txt">上次登录时间：</td>
                    <td bgcolor="#FFFFFF" class="left_txt">&nbsp;{# $last_login_time #}</td>
                  </tr>
                </table>
              </td>
            </tr>
            <tr>
              <td height="5" colspan="2">&nbsp;</td>
            </tr>
          </table>
        </td>
      </tr>

      
      <tr>
        <td height="40" colspan="2">
          <table width="100%" height="1" border="0" cellpadding="0" cellspacing="0" bgcolor="#CCCCCC">
            <tr>
              <td></td>
            </tr>
          </table>
        </td>
      </tr>
      
      <tr>
        <td width="2%">&nbsp;</td>
        <td width="51%" class="left_txt">
          <img src="images/icon-mail2.gif" width="16" height="11"> 客服邮箱 :001@xiaofz.com<br>
          <img src="images/icon-phone.gif" width="17" height="14"> 官方网站：<a href="http://www.xiaofz.com/blog">http://www.xiaofz.com/blog</a>
        </td>


      </tr>
      
    </table>
  </td>
  <td background="images/mail_rightbg.gif">&nbsp;</td>
</tr>

<tr>
  <td valign="bottom" background="images/mail_leftbg.gif"><img src="images/buttom_left2.gif" width="17" height="17" /></td>
  <td background="images/buttom_bgs.gif"><img src="images/buttom_bgs.gif" width="17" height="17"></td>
  <td valign="bottom" background="images/mail_rightbg.gif"><img src="images/buttom_right2.gif" width="16" height="17" /></td>
</tr>

</table>

</body>
</html>